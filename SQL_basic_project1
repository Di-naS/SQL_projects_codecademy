CREATE TABLE friends (
id INTEGER,
name TEXT,
birthday DATE);
INSERT INTO friends (id, name, birthday)
VALUES (1, 'Jane Doe', '1990-05-30');
SELECT * FROM friends;
INSERT INTO friends (id, name, birthday)
VALUES (2, 'Mary Stuart', '1985-04-28');
INSERT INTO friends (id, name, birthday)
VALUES (3, 'John Grey', '1992-02-03');
UPDATE friends
SET name = 'Jane Smith'
WHERE id = 1;
ALTER TABLE friends
ADD COLUMN email TEXT;
UPDATE friends
SET email = 'jane@codecademy.com'
WHERE id = 1;
UPDATE friends
SET email = 'mary.stuart@yahoo.com'
WHERE id = 2;
UPDATE friends
SET email = 'john.grey@live.com'
WHERE id = 3;
DELETE FROM friends
WHERE id = 1;
SELECT * FROM friends;




